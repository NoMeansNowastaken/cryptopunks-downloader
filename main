import requests

i = 1
t = 0
for x in range(10000):
    url = f'https://www.larvalabs.com/public/images/cryptopunks/punk{i}.png'
    r = requests.get(url=url)
    if r.status_code == 200:
        with open(f'punks/punk{i}.png', 'wb') as f:
            f.write(r.content)
        print(f'Saved punk {i}')
        t += 1
        print(f'Total saved: {t}')
    i += 1
